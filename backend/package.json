{
    "name": "card-journey-tracker-api",
    "version": "1.0.0",
    "description": "Card Journey Tracker Backend API with MongoDB Atlas - Complete card lifecycle tracking and analytics",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "test": "jest --watchAll=false",
        "test:watch": "jest --watchAll",
        "test:coverage": "jest --coverage",
        "lint": "eslint . --ext .js",
        "lint:fix": "eslint . --ext .js --fix",
        "analyze": "node -e \"require('./services/analyticsService').analyzeBottlenecks()\"",
        "seed": "node scripts/seedData.js",
        "migrate": "node scripts/migrate.js",
        "docs": "jsdoc -c jsdoc.conf.json",
        "start:prod": "NODE_ENV=production node server.js"
    },
    "keywords": [
        "card-tracking",
        "fintech",
        "api",
        "mongodb",
        "websocket",
        "analytics",
        "embossing",
        "delivery-tracking"
    ],
    "author": "Card Journey Tracker Team",
    "license": "MIT",
    "dependencies": {
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "express": "^4.18.2",
        "express-rate-limit": "^6.8.0",
        "helmet": "^7.0.0",
        "mongodb": "^6.18.0",
        "mongoose": "^7.5.0",
        "morgan": "^1.10.0",
        "node-cron": "^3.0.2",
        "uuid": "^9.0.0",
        "ws": "^8.13.0"
    },
    "devDependencies": {
        "@types/node": "^20.4.5",
        "eslint": "^8.46.0",
        "eslint-config-node": "^4.1.0",
        "eslint-plugin-node": "^11.1.0",
        "jest": "^29.6.2",
        "jsdoc": "^4.0.2",
        "nodemon": "^3.0.1",
        "supertest": "^6.3.3"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/your-org/card-journey-tracker-api.git"
    },
    "bugs": {
        "url": "https://github.com/your-org/card-journey-tracker-api/issues"
    },
    "homepage": "https://github.com/your-org/card-journey-tracker-api#readme"
}
